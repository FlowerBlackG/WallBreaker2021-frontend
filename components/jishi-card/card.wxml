<!--components/jishi-card/card.js.wxml-->
<view class="card">
<view class="tag" > 
    {{labelText}}
 </view>
 <view class="title">{{title}}
 </view>


 <view class="rightTag">
    我发布的
 </view>
 <view >
  <image src="/static/icon/default-user.png" class="icon" mode="scaleToFill"/>
</view>
<view>
    <text class="userName">{{userName}}
    </text>
</view>
<view>
    <text class="time">{{publishTime}}</text>
</view>
<view>
    <text class="description">{{description}}</text>
</view>
<view class="image">
    <image ></image>
</view>
</view>



