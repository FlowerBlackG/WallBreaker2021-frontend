/* 背景相关 */
.jishi-background,.jiren-background{
/* position */
position: absolute;
left: 0%;
right: 0%;
top: 0%;
bottom: 0%;
}
.jishi-background{
/* gradient_background */
background: linear-gradient(199.3deg, rgba(0, 193, 255, 0.3) -7.32%, rgba(0, 193, 255, 0) 27.37%), linear-gradient(198.24deg, rgba(140, 26, 255, 0.25) 3.34%, rgba(185, 115, 255, 0) 90.78%), linear-gradient(195.29deg, #A3E5FA 2.51%, #DCBCFB 56.49%);
}
.jiren-background{
  background: linear-gradient(199.75deg, rgba(140, 26, 255, 0.18) 40.18%, rgba(140, 26, 255, 0) 90.9%);
  background: linear-gradient(195.29deg, #DCBCFB 2.51%, #A2E4FA 27.51%);
}




/* 顶部导航栏相关（重载） */
.weui-navigation-bar__centers{
  font-family: PingFang SC;
  font-style: normal;
  font-weight: bold;
  font-size: 18px;
  line-height: 25px;
  display: flex;
  align-items: center;
  text-align: center;
  letter-spacing: 0.25vw;
  color: #FFFFFF;
  }
  .weui-search-bar {
    background: rgba(242, 252, 255, 0.35);
    mix-blend-mode: overlay;
    border-radius: 44px;
    left:2vw;
    width:96vw;
  }
  .weui-search-bar__form{
    height: 24px;
  }
  .weui-search-bar__box{
    height: 24px;
  }
  .weui-search-bar__box .weui-search-bar__input{
    padding:0 0 0 0 ;
    height: 24px;
  }



